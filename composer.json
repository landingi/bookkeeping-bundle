{
    "name": "landingi/bookkeeping-bundle",
    "description": "Landingi Bookkeeping Library",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "ext-simplexml": "*",
      "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "landingi/php-coding-standards": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Landingi\\BookkeepingBundle\\" : "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Landingi\\BookkeepingBundle\\" : "tests"
        }
    },
    "minimum-stability": "stable"
}
