{
    "name": "landingi/bookkeeping-bundle",
    "description": "Landingi Bookkeeping Library",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "ext-simplexml": "*",
        "ext-curl": "*",
        "ext-json": "*",
        "dragonbe/vies": "2.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "landingi/php-coding-standards": "1.0.4",
        "symfony/var-dumper": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "Landingi\\BookkeepingBundle\\" : "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Landingi\\BookkeepingBundle\\" : "tests"
        }
    },
    "minimum-stability": "stable"
}
